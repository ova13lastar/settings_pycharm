<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add">
        <Item>
          <IdeaActionID>Git.Add</IdeaActionID>
          <ShortCut>'Ctrl+Alt+A'</ShortCut>
          <Description>Add</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>15</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Source">
        <Item>
          <IdeaActionID>EditSource</IdeaActionID>
          <ShortCut>'F4'</ShortCut>
          <Description>Jump to Source</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Push...">
        <Item>
          <IdeaActionID>Vcs.Push</IdeaActionID>
          <ShortCut>'Ctrl+Maj+K'</ShortCut>
          <Description>Push...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'pytest in test_gaia....'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+Maj+F10'</ShortCut>
          <Description>Run 'pytest in test_gaia....'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'pytest in test_gaia.py'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Maj+F10'</ShortCut>
          <Description>Run 'pytest in test_gaia.py'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update Project...">
        <Item>
          <IdeaActionID>Vcs.UpdateProject</IdeaActionID>
          <ShortCut>'Ctrl+T'</ShortCut>
          <Description>Update Project...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="VCS Operations Popup...">
        <Item>
          <IdeaActionID>Vcs.QuickListPopupAction</IdeaActionID>
          <ShortCut>'Alt+`'</ShortCut>
          <Description>VCS Operations Popup...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Maj+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>